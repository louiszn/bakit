{
	"name": "bakit",
	"version": "3.0.1",
	"description": "A modern, full-stack Discord API wrapper and framework",
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"scripts": {
		"build": "tsdown --config-loader unrun",
		"type-check": "tsc --noEmit",
		"test": "vitest run --pass-with-no-tests"
	},
	"bin": {
		"bakit": "./dist/cli.js"
	},
	"files": [
		"dist"
	],
	"homepage": "https://github.com/louiszn/bakit#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/louiszn/bakit.git"
	},
	"bugs": {
		"url": "https://github.com/louiszn/bakit/issues"
	},
	"keywords": [
		"discord-bot",
		"bakit",
		"framework"
	],
	"author": "louiszn",
	"license": "MIT",
	"packageManager": "pnpm@10.15.0",
	"optionalDependencies": {
		"tsx": "^4.21.0"
	},
	"dependencies": {
		"@bakit/gateway": "workspace:^",
		"@bakit/rest": "workspace:^",
		"@bakit/service": "workspace:^",
		"@bakit/utils": "workspace:^",
		"@discordjs/collection": "^2.1.1",
		"commander": "^14.0.3",
		"discord-api-types": "^0.38.38",
		"dotenv": "^17.2.3",
		"tiny-glob": "^0.2.9",
		"tiny-mixin": "^1.1.1",
		"type-fest": "^4.41.0"
	}
}
