{
	"name": "bakit",
	"version": "2.1.2",
	"description": "A framework for discordeno",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./cli": "./dist/cli.js"
	},
	"scripts": {
		"build": "tsup",
		"type-check": "tsc --noEmit",
