		"discord-bot",
		"bakit",
		"framework"
	],
	"author": "louiszn",
	"license": "MIT",
	"packageManager": "pnpm@10.15.0",
	"optionalDependencies": {
		"tsx": "^4.21.0"
	},
	"dependencies": {
		"@bakit/gateway": "workspace:^",
		"@bakit/rest": "workspace:^",
		"@bakit/service": "workspace:^",
		"@bakit/utils": "workspace:^",
		"commander": "^14.0.2",
		"dotenv": "^17.2.3",
		"tiny-glob": "^0.2.9",
		"type-fest": "^4.41.0"
